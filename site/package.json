    "babel-polyfill": "^6.26.0",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.15.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-share": "^2.2.0",
    "react-spinners": "^0.3.2",
    "redux": "^4.0.0",
    "redux-async-await": "^1.0.1",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "semantic-ui-react": "^0.80.0",
    "truefit-react-utils": "^0.0.14"
  },
  "devDependencies": {
